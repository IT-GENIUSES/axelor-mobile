    "i18n-parser": "^1.0.0",
    "i18next": "^21.8.3",
    "react": "17.0.2",
    "react-i18next": "^11.16.7",
    "react-native": "0.68.1",
    "react-native-datawedge-intents": "^0.1.8",
    "react-native-device-info": "^9.0.2",
    "react-native-file-viewer": "^2.1.5",
