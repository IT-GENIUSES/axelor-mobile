{
  "name": "aos-mobile",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/apps/*",
      "example"
    ],
    "nohoist": [
      "**/example",
      "**/example/**"
    ]
  },
  "scripts": {
    "clean": "find . -type d -name node_modules | xargs rm -rf && rm -rf yarn.lock && find . -type d -name lib | xargs rm -rf",
    "dev": "nx run-many --target=dev --maxParallel=10 ",
    "build": "nx run-many --target=build",
    "i18n": "nx run-many --target=i18n",
    "release": "nx run-many --target=npm-publish --verbose",
    "test": "nx test example",
    "lint": "nx run-many --target=lint",
    "format": "nx run-many --target=format",
    "format:check": "nx run-many --target=format:check",
    "start": "nx start example",
    "android": "nx android example",
    "graph": "nx graph",
    "license:add": "nx run-many --target=license:add"
  },
  "devDependencies": {
    "@babel/runtime": "^7.19.0",
    "nx": "16.4.2",
    "typescript": "5.1.6"
  },
  "resolutions": {
    "@types/react": "18.2.0"
  
  }
}
